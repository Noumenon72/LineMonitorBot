<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
   <style name="UserEntered">
       <item name="android:textColor">@color/win8_green</item>
   </style>
   <style name="ProgramCalculated">
       <item name="android:textColor">@color/DodgerBlue</item>
   </style>  
   <style name="ProgramGuessed">
       <item name="android:textColor">@color/win8_orange</item>
   </style>
   
   <style name="Label" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>   
   </style>
   
   <style name="ParentFragment">
       <item name="android:paddingTop">@dimen/fragment_vertical_margin</item>
       <item name="android:paddingBottom">@dimen/fragment_vertical_margin</item>
       <item name="android:paddingLeft">@dimen/fragment_horizontal_margin</item>
       <item name="android:paddingRight">@dimen/fragment_horizontal_margin</item>  
       <item name="android:focusable">true</item>  
       <item name="android:focusableInTouchMode">true</item>
   </style>
   <style name="ParentFragment.HorizontallyDivided">
       <item name="android:divider">?android:attr/dividerHorizontal</item>
       <item name="android:dividerPadding">@dimen/fragment_divider_padding</item>
       <item name="android:showDividers">middle</item> 
   </style>
   
   <style name="Spinner" parent="@android:attr/spinnerItemStyle">
       <item name="android:gravity">center</item>
       <item name="android:textColor">@color/Sienna</item>
       <item name="android:layout_weight">0.4</item>
   </style>
   
   <style name="Label.FirstTableColumn">
       <item name="android:layout_gravity">left|center_vertical</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:layout_width">0dip</item>
       <item name="android:layout_weight">0.35</item>
       <item name="android:minHeight">70px</item>
       <item name="android:paddingRight">@dimen/table_column_padding</item>
   </style>
   <style name="Label.SecondTableColumn">
       <item name="android:gravity">center</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:layout_width">0dip</item>
       <item name="android:layout_weight">0.3</item>
       <item name="android:minHeight">70px</item>
   </style>
   <style name="EditText.SecondTableColumn">
       <item name="android:gravity">center</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:layout_width">0dip</item>
       <item name="android:layout_weight">0.3</item>
       <item name="android:minHeight">70px</item>
       <item name="android:ems">5</item>
   </style>
   <style name="BlankThirdColumn">
       <item name="android:layout_width">0dip</item>
       <item name="android:layout_height">wrap_content</item>
       <item name="android:layout_weight">0.15</item>
   </style>
   <style name="BlankThirdColumn.Unused">
       <item name="android:layout_weight">0.0</item>
   </style>
       
      
</resources>
